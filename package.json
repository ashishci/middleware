{
  "name": "middleware",
  "version": "1.0.0",
  "description": "custom middleware shared between projects",
  "main": "src/main.ts",
  "repository": "https://github.com/ashishci/middleware",
  "author": "Ash Chatterjee <ashish@ashishc.io>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev src/main.ts",
    "lint": "eslint --ignore-path .eslintignore --ext \"src/*.ts\"",
    "lint:fix": "eslint --ignore-path .eslintignore --ext \"src/*.ts\" --fix",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.+(js|ts|json)\"",
    "lint:format": "yarn lint && yarn format",
    "start": "ts-node src/main.ts",
    "redis:dev": "cross-env NODE_ENV=development ts-node-dev src/redis-cache/index.ts",
    "test": "yarn lint:format && jest",
    "build": "yarn lint:format && tsc -p tsconfig.build.json",
    "build:redis-cache": "ts-node  build.ts --target redis-cache --platform node --bundle --minify"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/commander": "^2.12.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@types/pg": "^8.6.6",
    "@types/redis-mock": "^0.17.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.8",
    "esbuild-node-externals": "^1.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-auto-spies": "^1.6.10",
    "path": "^0.12.7",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/jest": "^29.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.4.2",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-typeahead": "^2.2.2",
    "pg": "^8.9.0",
    "redis": "^4.6.4",
    "ts-jest": "^29.0.5",
    "winston": "^3.8.2"
  }
}
